{
  "manifest_version": 2,
  "name": "Github Godoc",
  "description": "Enhances Github UI for Go projects",
  "version": "1.4",
  "permissions": [
    "*://*.godoc.org/*",
    "tabs"
  ],
  "background": {
    "scripts": [
	    "background.js"
    ],
    "persistent": false
  },
  "web_accessible_resources": [
	  "images/gopher.png"
  ],
  "content_scripts": [
    {
      "matches": [
	      "https://github.com/*"
      ],
      "js": [
	      "content.js"
      ]
    }
  ],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
   "128": "images/icon128.png"
  }
}
